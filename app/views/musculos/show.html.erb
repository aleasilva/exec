<%- model_class = Musculo -%>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @musculo.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:observacao) %>:</strong></dt>
  <dd><%= @musculo.observacao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:selecionado) %>:</strong></dt>
  <dd><%= @musculo.selecionado ? 'Sim': 'NÃ£o' %></dd>

</dl>

<div class="panel-footer">

  <%= link_to t('.back', :default => t("helpers.links.back")),
              musculos_path, :class => 'btn btn-primary'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_musculo_path(@musculo), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              musculo_path(@musculo),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirmdelete", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
