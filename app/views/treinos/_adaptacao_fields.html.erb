<tr class="fields">
	<td><%= f.text_field :semana    ,:class=>"input-mini", :alt=>"intDecimal"%></td>
	<td><%= f.text_field :serie     ,:class=>"input-mini", :alt=>"intDecimal"%></td>
	<td><%= f.text_field :repeticao ,:class=>"input-mini", :alt=>"intDecimal"%></td>
	<td><%= f.text_field :carga     ,:class=>"input-mini", :alt=>"intDecimal"%></td>
	<td><%= f.text_field :intervalo ,:class=>"input-mini", :alt=>"intDecimal"%></td>
	<td><%= f.text_field :observacao,:class=>"input", :alt=>"intDecimal"%></td>
	<td class="form-inline">
		<%= f.collection_check_boxes :diasemana_ids, @aluno.diasemanas, :id, :nomeabreviado%>
	</td>
	<td><%= link_to_remove_fields "Excluir", f %></td>
