<p id="notice"><%= notice %></p>
<h1>Mostrando Treino</h1>
<p>
  	<strong>Criacao:</strong>
	<%= @treino.criacao %>
</p>
<p>
  	<strong>Validade:</strong>
	<%= @treino.validade %>
</p>
<p>
  	<strong>Observacao:</strong>
	<%= @treino.observacao %>
</p>

<hr>

<p>  
  <strong>Lista de atividades:</strong>
  <br>
	<% @treino.atividades.each do |iAtividade| %>
    	<%= iAtividade.nome %>
    	<br>    	
    	<% iAtividade.musculos.each do |iMusculo| %>
    		<%= iMusculo.nome %>
		<% end %> 
    	<br>
	<% end %> 
</p>

<%= link_to 'Edit', edit_treino_path(@treino) %> |
