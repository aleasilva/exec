<%- model_class = Treino -%>


<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:criação) %>:</strong></dt>
  <dd><%= @treino.criacao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:validade) %>:</strong></dt>
  <dd><%= @treino.validade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:observação) %>:</strong></dt>
  <dd><%= @treino.observacao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:aluno_id) %>:</strong></dt>
  <dd><%= @treino.aluno_id %></dd>
</dl>

<div class="panel-footer">
  <%= link_to t('.back', :default => t("helpers.links.back")),treinos_path, :class => 'btn btn-primary'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),edit_treino_path(@treino), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              treino_path(@treino),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirmdelete", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
