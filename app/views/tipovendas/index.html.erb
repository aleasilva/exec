<p id="notice"><%= notice %></p>

<h1>Listing Tipovendas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipovendas.each do |tipovenda| %>
      <tr>
        <td><%= tipovenda.nome %></td>
        <td><%= link_to 'Show', tipovenda %></td>
        <td><%= link_to 'Edit', edit_tipovenda_path(tipovenda) %></td>
        <td><%= link_to 'Destroy', tipovenda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipovenda', new_tipovenda_path %>
