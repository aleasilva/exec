<%- model_class = Tipovenda -%>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @tipovenda.nome %></dd>
</dl>

<div class="panel-footer">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              tipovendas_path, :class => 'btn btn-primary'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tipovenda_path(@tipovenda), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tipovenda_path(@tipovenda),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirmdelete", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
